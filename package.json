{
  "name": "chip-warmup-api",
  "version": "1.0.0",
  "description": "Sistema completo de aquecimento de chip com API para clientes Android - PostgreSQL Edition",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "echo 'Backend build complete'",
    "install:all": "npm install && cd frontend && npm install",
    "install:quick": "node scripts/install-dependencies.js",
    "install:universal": "node install-universal.js",
    "install:test": "node test-installation.js",
    "setup:db": "node scripts/setup-postgresql.js",
    "setup:full": "npm run install:quick && npm run setup:db",
    "test": "jest",
    "test:api": "node test-api.js",
    "test:installation": "node test-installation.js",
    "test:integration": "node scripts/test-android-integration.js",
    "create-tasks": "node scripts/create-warmup-tasks.js",
    "create-sample": "node scripts/create-sample-tasks.js",
    "create-real-data": "node scripts/create-real-data.js",
    "create-sample-content": "node scripts/create-sample-content.js",
    "create-sample-sendable-content": "node scripts/create-sample-sendable-content.js",
    "setup": "node scripts/setup.js",
    "dev:full": "concurrently \"cross-env PORT=3001 npm run dev\" \"cd frontend && npm start\"",
    "frontend": "cd frontend && npm start",
    "frontend:build": "cd frontend && npm run build",
    "frontend:install": "cd frontend && npm install",
    "lint": "eslint .",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:clean": "docker-compose down -v --remove-orphans",
    "health:check": "node healthcheck.js",
    "check:errors": "node scripts/check-errors.js",
    "backup:create": "node scripts/backup-database.js",
    "backup:restore": "node scripts/restore-database.js",
    "package": "node scripts/package.js",
    "package:zip": "node scripts/package.js --format=zip",
    "package:tar": "node scripts/package.js --format=tar",
    "package:exe": "node scripts/package.js --format=exe",
    "package:win": "node scripts/package.js --platform=win --format=zip",
    "package:linux": "node scripts/package.js --platform=linux --format=zip",
    "package:mac": "node scripts/package.js --platform=mac --format=zip",
    "package:all": "node scripts/package.js --platform=all --format=zip",
    "package:minimal": "node scripts/package.js --minimal --format=zip",
    "package:prod": "node scripts/package.js --production --format=zip",
    "package:full": "node scripts/package.js --include-deps --production --format=zip",
    "release": "node scripts/create-release.js",
    "release:win": "node scripts/create-release.js --platform=win",
    "release:linux": "node scripts/create-release.js --platform=linux",
    "release:mac": "node scripts/create-release.js --platform=mac",
    "release:all": "node scripts/create-release.js --platform=all",
    "release:prod": "node scripts/create-release.js --production",
    "release:full": "node scripts/create-release.js --include-deps --production",
    "install:vps": "node scripts/install-vps.js",
    "install:vps:auto": "chmod +x scripts/install-vps.sh && sudo ./scripts/install-vps.sh",
    "fix:vps": "node scripts/fix-vps-installation.js",
    "migrate:postgres": "node scripts/setup-postgresql.js"
  },
  "keywords": [
    "chip-warmup",
    "android-api",
    "task-management",
    "automation",
    "postgresql"
  ],
  "author": "Chip Warmup Team",
  "license": "MIT",
  "dependencies": {
    "exceljs": "^4.4.0",
    "pdfkit": "^0.14.0",
    "csv-writer": "^1.6.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "nodemailer": "^6.9.7",
    "joi": "^17.11.0",
    "rate-limiter-flexible": "^2.4.2",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "axios": "^1.6.2",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
} 